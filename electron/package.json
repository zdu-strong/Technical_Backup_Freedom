{
  "name": "my-app",
  "version": "0.1.0",
  "scripts": {
    "start": "npx -y -p typescript -p ts-node ts-node --skipProject bin/pre_load_configuration.ts && ts-node --skipProject bin/start.ts",
    "pack": "npx -y -p typescript -p ts-node ts-node --skipProject bin/pre_load_configuration.ts && ts-node --skipProject bin/pack.ts",
    "make": "npx -y -p typescript -p ts-node ts-node --skipProject bin/pre_load_configuration.ts && ts-node --skipProject bin/make.ts"
  },
  "main": "./dist/index.js",
  "private": true,
  "homepage": ".",
  "build": {
    "extends": null,
    "files": [
      "dist",
      "public",
      "build"
    ],
    "directories": {
      "output": "output"
    },
    "appId": "com.electron.electron",
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "deleteAppDataOnUninstall": true,
      "guid": "b5a96e2b-5f4f-4a67-b77a-2c9d7c947242",
      "runAfterFinish": true,
      "allowToChangeInstallationDirectory": true
    },
    "asar": {
      "smartUnpack": true
    },
    "asarUnpack": [
      "public",
      "build",
      "node_modules/@electron/remote",
      "node_modules/node-os-utils"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {
      "import/no-anonymous-default-export": [
        "off",
        {
          "allowArrowFunction": true
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      "last 1 chrome version"
    ],
    "development": [
      "last 1 chrome version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "7.21.11",
    "@babylonjs/core": "6.39.0",
    "@emotion/react": "11.11.3",
    "@emotion/styled": "11.11.0",
    "@fontsource/roboto": "5.0.8",
    "@fortawesome/free-brands-svg-icons": "6.5.1",
    "@fortawesome/free-solid-svg-icons": "6.5.1",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@mui/lab": "5.0.0-alpha.161",
    "@mui/material": "5.15.5",
    "@mui/x-date-pickers": "6.19.0",
    "@nestjs/cli": "10.3.0",
    "@types/crypto-js": "4.2.1",
    "@types/node": "20.11.5",
    "@types/node-os-utils": "1.3.4",
    "@types/node-rsa": "1.1.4",
    "@types/qs": "6.9.11",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@types/react-router-dom": "5.3.3",
    "@types/uuid": "9.0.7",
    "@types/wait-on": "5.3.4",
    "autoprefixer": "10.4.17",
    "crypto-js": "4.2.0",
    "dexie": "3.2.4",
    "electron": "28.1.4",
    "electron-builder": "24.9.1",
    "eslint": "8.56.0",
    "execa": "5.1.1",
    "get-port": "5.1.1",
    "is-mobile": "4.0.0",
    "jsencrypt": "3.3.2",
    "node-polyfill-webpack-plugin": "3.0.0",
    "node-rsa": "1.1.1",
    "postcss": "8.4.33",
    "react-app-rewired": "2.2.1",
    "react-git-info": "2.0.1",
    "react-intl": "6.6.1",
    "react-router-dom": "6.21.3",
    "react-scripts": "5.0.1",
    "rxjs-exhaustmap-with-trailing": "2.1.1",
    "source-map-support": "0.5.21",
    "tailwindcss": "3.4.1",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "4.9.5",
    "typestyle": "2.4.0",
    "wait-on": "7.2.0",
    "web-vitals": "3.5.1",
    "webworker-promise": "0.5.1",
    "windows-locale": "1.1.3"
  },
  "dependencies": {
    "@electron/remote": "2.1.1",
    "axios": "1.6.5",
    "date-fns": "2.30.0",
    "electron-log": "5.0.3",
    "linq": "3.2.4",
    "mathjs": "12.3.0",
    "mobx-react-use-autorun": "4.0.20",
    "node-os-utils": "1.3.7",
    "qs": "6.11.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "reflect-metadata": "0.2.1",
    "rxjs": "7.8.1",
    "stream-to-string": "1.2.1",
    "tree-kill": "1.2.2",
    "typedjson": "1.8.0",
    "uuid": "9.0.1"
  }
}
